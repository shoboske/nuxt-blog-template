    "@nuxtjs/style-resources": "^1.2.1",
    "animate.css": "^4.1.1",
    "core-js": "^3.19.3",
    "eslint-plugin-prettier": "^4.0.0",
    "esm": "^3.2.25",
    "gray-matter": "^4.0.3",
